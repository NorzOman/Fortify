{
    "version": 2,
    "builds": [
        {
            "src": "backend-api/app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "backend-api/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "backend-api/app.py"
        }
    ]
}
